{
    "configurations": [
        {
            "name": "ESP32",
            "compilerPath": "~/.arduino15/packages/esp32/tools/xtensa-esp32-elf-gcc/*/bin/xtensa-esp32-elf-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "includePath": [
                "${workspaceFolder}/**",
                "~/.arduino15/packages/esp32/hardware/esp32/*/cores/esp32/**",
                "~/.arduino15/packages/esp32/hardware/esp32/*/libraries/**",
                "~/.arduino15/packages/esp32/hardware/esp32/*/variants/esp32/**",
                "~/.arduino15/packages/esp32/hardware/esp32/*/tools/sdk/esp32/include/**",
                "~/.arduino15/packages/esp32/tools/xtensa-esp32-elf-gcc/*/xtensa-esp32-elf/include/**",
                "~/Arduino/libraries/**"
            ],
            "defines": [
                "PLATFORMIO=60111",
                "ESP32",
                "ESP_PLATFORM",
                "ARDUINO_ARCH_ESP32",
                "ARDUINO_ESP32_DEV",
                "ARDUINO_BOARD=\"ESP32_DEV\"",
                "ARDUINO_VARIANT=\"esp32\"",
                "ARDUINO_PARTITION_default",
                "F_CPU=240000000L",
                "ARDUINO=10819",
                "ARDUINO_USB_CDC_ON_BOOT=0",
                "CORE_DEBUG_LEVEL=0",
                "__XTENSA__"
            ],
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-mlongcalls",
                "-Wno-frame-address"
            ]
        }
    ],
    "version": 4
}

